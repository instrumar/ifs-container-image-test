{
  "tenant": "public",
  "namespace": "default",
  "name": "iceberg-test-sink",
  "inputs": ["public/default/test-iceberg-pulsar"],
  "archive": "connectors/pulsar-io-iceberg-0.1.0.nar",
  "parallelism": 1,
  "configs": {
    "format": "parquet",
    "catalog": "hadoop",
    "catalog-impl": "org.apache.iceberg.hadoop.HadoopCatalog",
    "warehouse-location": "s3a://test-dev-us-west-2/lakehouse/iceberg_sink",
    "table-name": "test_iceberg_table",
    "s3a.access.key": "<your-minio-access-key>",
    "s3a.secret.key": "<your-minio-secret-key>",
    "s3a.endpoint": "<your-minio-endpoint>",
    "s3a.path.style.access": true
  }
}
